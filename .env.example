# Spacetime Visualizer Configuration
# Copy this file to .env to customize your local environment
# All values are optional - safe defaults will be used if not set

# Grid resolution for spacetime visualization (cells per axis)
# Default: 32
# Valid range: 8-256 (higher values require more GPU memory)
VITE_GRID_RESOLUTION=32

# Animation timestep in seconds
# Default: 0.016 (~60 FPS)
# Valid range: 0.001-0.1 (lower values = smoother animation, higher CPU usage)
VITE_ANIMATION_TIMESTEP=0.016
